{
    "name": "notpesto-bot",
    "version": "0.1.0",
    "description": "Like said, it's not Pesto.",
    "main": "app.js",
    "type": "module",
    "scripts": {
        "start": "ts-node --esm ./app.ts",
        "dev": "nodemon --esm app.ts",
        "lint": "eslint --fix --ext .ts && prettier --write ./**/*.ts",
        "prepare": "husky install"
    },
    "author": "ZoidC",
    "license": "MIT",
    "dependencies": {
        "@keyv/mongo": "^2.1.8",
        "discord.js": "^14.7.1",
        "dotenv": "^16.0.3",
        "keyv": "^4.5.2"
    },
    "devDependencies": {
        "@types/node": "^18.14.0",
        "@typescript-eslint/eslint-plugin": "^5.52.0",
        "@typescript-eslint/parser": "^5.52.0",
        "eslint": "^8.34.0",
        "eslint-config-prettier": "^8.6.0",
        "husky": "^8.0.3",
        "lint-staged": "^13.1.2",
        "nodemon": "^2.0.20",
        "prettier": "^2.8.4",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    },
    "lint-staged": {
        "*.ts": "eslint --cache --fix",
        "*.{ts,css,md}": "prettier --write"
    }
}